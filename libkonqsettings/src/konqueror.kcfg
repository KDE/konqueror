  <group name="MainView Settings" >
<!-- htmlopts.cpp -->
    <!-- TODO Settings: can't be applied to WebEnginePart without a lot of work. Decide what to do. At least hide widgets until implemented -->
    <entry key="OpenMiddleClick" type="Bool"> <!--KHTML-->
      <default>true</default>
      <label></label>
      <whatsthis></whatsthis>
      <!-- checked -->
    </entry>
    <!-- TODO Settings: can't be applied to WebEnginePart without a lot of work. Decide what to do. At least hide widgets until implemented -->
    <entry key="BackRightClick" type="Bool"> <!--konqmouseeventfilter.cpp but also in KHTML !?-->
      <default>false</default>
      <label></label>
      <whatsthis></whatsthis>
      <!-- checked -->
    </entry>
    <entry key="OfferToSaveWebsitePassword" type="Bool">
      <default>true</default>
    </entry>
<!-- konqguiclients.cpp -->
    <entry key="ToggableViewsShown" type="StringList">
      <label></label>
      <whatsthis></whatsthis>
      <!-- checked -->
    </entry>
  </group>

  <group name="HistorySettings" >
    <!-- TODO Settings: see whether it is possible to remove the KonqHistorySettings class in konqhistorysettings.{cpp,h} and use this class instead -->
<!-- history_module.cpp -->
    <entry key="OpenGroups" type="String">
      <label></label>
      <whatsthis></whatsthis>
    </entry>
<!-- konqview.cpp -->
    <entry key="MaximumHistoryEntriesPerView" type="Int">
      <label>Maximum number of history items per view</label>
      <default>50</default>
      <whatsthis></whatsthis>
    </entry>
    <entry key="Default Action" name="defaultAction" type="Int">
      <!-- TODO Settings: see whether it's possible to replace this with an enum -->
      <default>0</default>
    </entry>
    <entry key="Maximum age of History entries" name="maximumHistoryEntryAge" type="Int">
      <default>90</default>
    </entry>
    <!-- TODO Settings: add other settings after removing the KonqHistorySettings class -->
  </group>

